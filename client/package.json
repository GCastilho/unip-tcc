{
  "name": "sapper",
  "description": "Sapper frontend",
  "version": "0.0.1",
  "scripts": {
    "start": "cd __sapper__/build && node index.js",
    "dev": "sapper dev",
    "build": "npm run build:clean && npm run build:sapper && npm run build:repository && npm run build:fix-configs",
    "build:clean": "del-cli ./__sapper__/build",
    "build:sapper": "sapper build",
    "build:repository": "copyfiles -E static/** package* __sapper__/build/",
    "build:fix-configs": "rexreplace \"__sapper__/build\" \".\" __sapper__/build/server/server.js && rexreplace \"cd __sapper__/build && \" \"\" __sapper__/build/package.json",
    "export": "sapper export",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "axios": "^0.20.0",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.5",
    "d3": "^6.2.0",
    "express": "^4.17.1",
    "light-date": "^1.0.1",
    "lodash": "^4.17.20",
    "qrious": "^4.0.2",
    "sirv": "^1.0.6",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.11.5",
    "@babel/preset-env": "^7.11.5",
    "@babel/runtime": "^7.11.2",
    "@rollup/plugin-babel": "^5.2.1",
    "@rollup/plugin-commonjs": "^15.0.0",
    "@rollup/plugin-node-resolve": "^9.0.0",
    "@rollup/plugin-replace": "^2.3.3",
    "@rollup/plugin-typescript": "^6.0.0",
    "@tsconfig/svelte": "^1.0.10",
    "@types/d3": "^5.16.3",
    "@types/lodash": "^4.14.163",
    "copyfiles": "^2.3.0",
    "del-cli": "^3.0.1",
    "npm-run-all": "^4.1.5",
    "rexreplace": "^6.0.3",
    "rollup": "^2.27.1",
    "rollup-plugin-svelte": "^6.0.1",
    "rollup-plugin-svelte-svg": "^0.2.3",
    "rollup-plugin-terser": "^7.0.2",
    "sapper": "^0.28.7",
    "svelte": "^3.25.1",
    "svelte-check": "^1.0.44",
    "svelte-preprocess": "^4.3.0",
    "typescript": "^4.0.2"
  }
}
