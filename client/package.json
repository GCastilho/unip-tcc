{
  "name": "sapper",
  "description": "Sapper frontend",
  "version": "0.0.1",
  "scripts": {
    "dev": "sapper dev",
    "build": "sapper build && copyfiles -E static/** package* __sapper__/build/ && rexreplace \"__sapper__/build\" \".\" __sapper__/build/server/server.js && rexreplace \"node __sapper__/build\" \"node index.js\" __sapper__/build/package.json",
    "export": "sapper export",
    "start": "node __sapper__/build",
    "cy:run": "cypress run",
    "cy:open": "cypress open",
    "test": "run-p --race dev cy:run"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "compression": "^1.7.1",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "qrious": "^4.0.2",
    "sirv": "^0.4.0",
    "socket.io-client": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.9.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/runtime": "^7.9.2",
    "@rollup/plugin-commonjs": "^11.0.2",
    "@rollup/plugin-node-resolve": "^7.1.1",
    "@rollup/plugin-replace": "^2.3.1",
    "copyfiles": "^2.3.0",
    "npm-run-all": "^4.1.5",
    "rexreplace": "^6.0.3",
    "rollup": "^2.3.3",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-svelte": "^5.2.1",
    "rollup-plugin-terser": "^5.3.0",
    "sapper": "^0.27.12",
    "svelte": "^3.20.1"
  }
}
